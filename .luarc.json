{
	"$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema-zh-cn.json",
	"diagnostics": {
		"globals": [
			"vim",
			"packer_plugins"
		]
	},
	"runtime": {
		"version": "LuaJIT"
	},
	"workspace": {
		"maxPreload": 100000,
		"preloadFileSize": 10000,
		"ignoreDir": [
			".vscode",
			".git"
		],
		"library": [
			"/usr/local/Cellar/neovim/0.7.0/share/nvim/runtime/lua",
			"/usr/local/Cellar/neovim/0.7.0/share/nvim/runtime/lua/vim/lsp"
		]
	},
	"telemetry": {
		"enable": false
	}
}
